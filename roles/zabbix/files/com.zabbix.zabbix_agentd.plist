<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.zabbix.zabbix_agentd</string>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<dict>
		<key>SuccessfulExit</key>
		<false/>
	</dict>
	<key>UserName</key>
	<string>zabbix</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/sbin/zabbix_agentd</string>
		<string>-c</string>
		<string>/usr/local/etc/zabbix/zabbix_agentd.conf</string>
		<string>-f</string>
	</array>
	<key>StandardErrorPath</key>
	<string>/tmp/zabbix_agentd-err.log</string>
	<key>StandardOutPath</key>
	<string>/tmp/zabbix_agentd-out.log</string>
</dict>
</plist>
